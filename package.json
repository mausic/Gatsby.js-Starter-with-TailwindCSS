{
  "name": "gatsby-tailwindcss-starter",
  "version": "1.0.0",
  "private": true,
  "description": "Gatsby.js TailwindCSS starter",
  "author": "Maksym Ryndia",
  "packageManager": "yarn@3.1.0",
  "keywords": [
    "gatsby",
    "tailwindcss",
    "clueby",
    "investors"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "prettify": "yarn prettier --write src/",
    "lint": "yarn eslint --fix --ext .js src/",
    "postinstall": "husky install",
    "upgrade": "yarn upgrade-interactive"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "gatsby": "^4.2.0",
    "gatsby-plugin-image": "^2.2.0",
    "gatsby-plugin-manifest": "^4.2.0",
    "gatsby-plugin-mdx": "^3.2.0",
    "gatsby-plugin-react-helmet": "^5.2.0",
    "gatsby-plugin-sharp": "^4.2.0",
    "gatsby-plugin-sitemap": "^5.2.0",
    "gatsby-source-filesystem": "^4.2.0",
    "gatsby-transformer-sharp": "^4.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "autoprefixer": "latest",
    "eslint": "^8.3.0",
    "eslint-config-google": "latest",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "latest",
    "gatsby-plugin-postcss": "^5.2.0",
    "husky": ">=6",
    "lint-staged": ">=10",
    "postcss": "latest",
    "prettier": "^2.4.1",
    "tailwindcss": "latest"
  },
  "lint-staged": {
    "*.js": "yarn eslint --cache --fix",
    "*.{js,css,md}": "yarn prettier --write"
  }
}
